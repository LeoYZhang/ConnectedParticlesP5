{
    "name": "connected-particles-p5",
    "version": "1.0.1",
    "description": "Connected particles simulation made in p5.js",
    "type": "module",
    "main": "dist/connected-particles-p5.umd.js",
    "module": "dist/connected-particles-p5.esm.js",
    "files": [
        "dist",
        "src"
    ],
    "exports": {
        ".": {
            "import": "./dist/connected-particles-p5.esm.js",
            "require": "./dist/connected-particles-p5.umd.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LeoYZhang/ConnectedParticlesP5.git"
    },
    "keywords": [
        "particles",
        "simulation",
        "p5js"
    ],
    "author": "Leo Zhang",
    "license": "ISC",
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "p5": "^1.9.0",
        "rollup": "^4.40.2",
        "rollup-plugin-copy": "^3.5.0"
    },
    "peerDependencies": {
        "p5": "^1.0.0"
    },
    "bugs": {
        "url": "https://github.com/LeoYZhang/ConnectedParticlesP5/issues"
    },
    "homepage": "https://github.com/LeoYZhang/ConnectedParticlesP5#readme"
}
